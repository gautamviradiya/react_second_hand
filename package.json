{
	"name": "react-native-second-hand",
	"displayName": "React native second hand",
	"description": "Make react native app development fast",
	"version": "1.0.0",
	"publisher": "gautamviradiya",
	"author": {
		"name": "Gautam virdiya",
		"email": "gautamvirdiya1@gmail.com"
	},
	"icon": "./src/assets/image/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/gautamviradiya/react_second_hand.git"
	},
	"bugs": {
		"url": "https://github.com/gautamviradiya/react_second_hand/issues",
		"email": "gautamviradiya1@gmail.com"
	},
	"keywords": [
		"react native snippets",
		"react native productivity",
		"react native add screen"
	],
	"galleryBanner": {
		"color": "#41205f",
		"theme": "dark"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other",
		"Snippets"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "gautamv.addScreen",
				"title": "Add Screen"
			}
		],
		"keybindings": [
			{
				"command": "gautamv.addScreen",
				"key": "alt+a"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "explorerResourceIsFolder",
					"command": "gautamv.addScreen",
					"group": "2_workspace"
				}
			]
		},
		"snippets": [
			{
				"language": "javascript",
				"path": "./src/snippats/long_code.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts"
	},
	"devDependencies": {
		"@types/vscode": "^1.61.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	}
}